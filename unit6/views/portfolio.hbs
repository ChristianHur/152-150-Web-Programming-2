<main>
    <h1 id="title">{{ title }}</h1>
    <section>
        <h1>The following are some projects I've developed.</h1>
        <dl>
            {{#each projects}}
                <dt>
                    <h3>{{title}}</h3>
                <dd>{{description}}</dd>
                <dd>Url: <a href="{{url}}" target="_blank">{{url}}</a></dd>
                </dt>
            {{else}}
                <dt>
                    <h3>Sorry, no data.</h3>
                </dt>
            {{/each}}            
        </dl>
        *These are just fictictious projects and sites.
    </section>
</main>